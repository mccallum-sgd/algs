import java.util.Iterator;
import java.util.NoSuchElementException;

public class RandomizedBag<Item> {

	private ResizingArray<Item> items;
	
	public RandomizedBag() {
		items = new ResizingArray<Item>();
	}
	
	public boolean isEmpty() {
		return items.size() == 0;
	}
	
	public int size() {
		return items.size();
	}
	
	public void put(Item item) {
		if (item == null) throw new NullPointerException();
		items.add(item);
	}
	
	public Item get() {
		if (isEmpty()) throw new NoSuchElementException();
	}
	
	public Item sample() {
		if (isEmpty()) throw new NoSuchElementException();
	}
	
	
	public Iterator<Item> iterator() {
		return new RandIterator(items);
	}
	
	private class RandIterator implements Iterator<Item> {
		private ResizingArray<Item> items;
		
		public RandIterator(ResizingArray<Item> items)

		@Override
		public boolean hasNext() {
			
			return false;
		}

		@Override
		public Object next() {
			// TODO Auto-generated method stub
			return null;
		}
		
	}
	
	public static void main(String[] args) {
		
	}

}
