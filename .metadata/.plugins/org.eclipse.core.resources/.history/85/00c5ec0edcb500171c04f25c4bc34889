import edu.princeton.cs.algs4.MaxPQ;

public class PQTester {

    public static void main (String[] args) {
        for (int i = 1; i < 5; i++)
            for (int j = 10; j < 1000000; j+=10) {
                MaxPQ<Integer> mpq = new MaxPQ<Integer>(j);
                randomFill(mpq);
                remove(mpq, mpq.size()/2);
                randomFill(mpq);
                remove(mpq, mpq.size());
            }
    }
    
    private static void randomFill(MaxPQ<Integer> q) {
        while (!q.isEmpty())
            q.insert(StdRandom.uniform(1000000, 10000000));
    }
    
    private static void remove(MaxPQ<Integer> q, int num) {
        for (int i = 0; i < num; i++)
            q.delMax();
    }
}
