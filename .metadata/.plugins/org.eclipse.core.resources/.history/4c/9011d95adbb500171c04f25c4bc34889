import edu.princeton.cs.algs4.MaxPQ;

public class PQTester {

    public static void main (String[] args) {
        for (int i = 10; i < 1000000; i+=10) {
            MaxPQ<Integer> mpq = new MaxPQ<Integer>(i);
            randomFill(mpq);
        }
        
    }
    
    private static void randomFill(MaxPQ<Integer> q) {
        while (!q.isEmpty())
            q.insert(StdRandom.uniform(1000000, 9999999));
    }
}
